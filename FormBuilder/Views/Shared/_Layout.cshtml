<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

    @*form io links*@
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <link href="https://bootswatch.com/4/cosmo/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/formiojs@latest/dist/formio.full.min.css">

    <link href="/formio.js/app/fontawesome/css/font-awesome.min.css" rel="stylesheet">

    <link href="~/Content/css/formio.css" rel="stylesheet">

    @*<script src="https://formio.github.io/formio.js/dist/formio.full.js"></script>*@
    <script src="~/Content/js/formio.full.js"></script>

    @*//end//*@

</head>
<body>
    <div class="navbar navbar-inverse">
        <div class="container-fluid">
            <nav class="navbar navbar-expand-sm bg-light">
                <ul class="navbar-nav">
                    <li class="nav-item" style="margin-right:10px;">@Html.ActionLink("Home", "List", "FormBuilder", new { @class = "nav-item" })</li>
                    <li class="nav-item" style="margin-right:10px;">@Html.ActionLink("Form Builder", "Index", "FormBuilder", null, new { @class = "nav-item" })</li>
                    <li class="nav-item" style="margin-right:10px;">@Html.ActionLink("Form List", "List", "FormBuilder", null, new { @class = "nav-item" })</li>
                    <li class="nav-item" style="margin-right:10px;">@Html.ActionLink("Data Filter", "DataFilter", "FormBuilder", null, new { @class = "nav-item" })</li>
                    <li class="nav-item" style="margin-right:10px;">@Html.ActionLink("About", "About", "Home", new { @class = "nav-item" })</li>
                    <li class="nav-item" style="margin-right:10px;">@Html.ActionLink("Contact", "Contact", "Home", new { @class = "nav-item" })</li>

                </ul>

            </nav>
        </div>
    </div>

    <div class="body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)

    
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>



    <script src="~/Content/js/formjs.js"></script>
</body>
</html>
